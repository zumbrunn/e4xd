var header = this.render(view);

var thispath = this.path.reverse();
thispath.pop();
for (var next in thispath) {
    var item = thispath[next];
    var itemtitle = item.title || item.name || 'Home';
    header += <span class="breadcrumb">
            <a href={item.href()}> > {itemtitle} </a>
        </span>;
}

return header;
